{
  "name": "grafana-sentry-datasource",
  "version": "1.0.1",
  "description": "Grafana data source plugin for Sentry",
  "main": "dist/module.js",
  "author": "Grafana Labs",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "scripts": {
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "start": "yarn watch",
    "test": "grafana-toolkit plugin:test",
    "e2e": "grafana-e2e run",
    "e2e:open": "grafana-e2e open",
    "e2e:update": "grafana-e2e run --update-screenshots",
    "build": "grafana-toolkit plugin:build",
    "ci-build": "grafana-toolkit plugin:ci-build"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@grafana/data": "8.4.7",
    "@grafana/runtime": "8.4.7",
    "@grafana/ui": "8.4.7",
    "lodash": "^4.17.21",
    "rxjs": "6.3.3",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@grafana/e2e": "8.4.7",
    "@grafana/e2e-selectors": "8.4.7",
    "@grafana/toolkit": "8.4.7",
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/lodash": "^4.14.175",
    "cspell": "^6.10.1",
    "cypress": "^7.7.0"
  },
  "resolutions": {
    "rxjs": "6.3.3"
  }
}
