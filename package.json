{
  "name": "grafana-sentry-datasource",
  "version": "2.2.3",
  "description": "Grafana data source plugin for Sentry",
  "main": "dist/module.js",
  "author": "Grafana Labs",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "scripts": {
    "build": "yarn build:client && yarn build:server",
    "build:client": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "build:server": "mage -v build:linux",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "dev_mock": "docker-compose -f docker-compose-mock.yml up",
    "e2e:playwright": "yarn playwright test",
    "e2e:playwright:ui": "yarn playwright test --ui",
    "e2e:playwright:report": "yarn playwright show-report",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "start": "yarn watch",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "e2e": "playwright test"
  },
  "dependencies": {
    "@emotion/css": "11.11.2",
    "@grafana/data": "12.0.2",
    "@grafana/plugin-ui": "^0.13.0",
    "@grafana/runtime": "12.0.2",
    "@grafana/schema": "12.0.2",
    "@grafana/ui": "12.0.2",
    "lodash": "^4.17.21",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@grafana/e2e-selectors": "12.0.2",
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "^3.0.0",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "1.58.1",
    "@swc/core": "^1.15.11",
    "@swc/helpers": "^0.5.10",
    "@swc/jest": "^0.2.36",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.21",
    "@types/node": "^20.19.27",
    "@types/react-router-dom": "^5.2.0",
    "copy-webpack-plugin": "^13.0.0",
    "cspell": "^9.4.0",
    "css-loader": "^7.0.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-webpack-plugin": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.8.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.97.3",
    "sass-loader": "14.1.1",
    "style-loader": "3.3.4",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "@stylistic/eslint-plugin-ts": "^4.0.0",
    "@types/testing-library__jest-dom": "5.14.9",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "imports-loader": "^5.0.0",
    "semver": "^7.7.2",
    "terser-webpack-plugin": "^5.3.15",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "resolutions": {
    "rxjs": "7.8.2",
    "jackspeak": "4.2.3"
  },
  "packageManager": "yarn@1.22.19"
}
